{
  "name": "micro-frm/framework",
  "description": "A PHP micro framework with minimal functionality for web apps",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "MicroFrm\\": "lib/micro-frm/"
    }
  },
  "require": {
    "php": ">=7.4",
    "symfony/yaml": "^5.4",
    "google/apiclient": "^2.12",
    "auth0/auth0-php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "guzzlehttp/psr7": "^2.0",
    "http-interop/http-factory-guzzle": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
